{
  "name": "sensibull-project",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Ravik Ganguly <theravikganguly@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf build && rm yarn.lock",
    "build": "tsc -p tsconfig.release.json",
    "watch:ts": "tsc -w -p tsconfig.release.json",
    "watch:node": "nodemon -r dotenv/config --delay 100ms build/src/index.js",
    "start": "node build/src/index.js",
    "start:qa": "concurrently -r \"npm run watch:ts\" \"npm run watch:node\""
  },
  "dependencies": {
    "@awaitjs/express": "^0.9.0",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@types/connect-timeout": "^0.0.35",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.177",
    "@types/node": "~12.12.27",
    "@types/pg": "^8.6.3",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "~2.19.2",
    "@typescript-eslint/parser": "~2.19.2",
    "concurrently": "5.3.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.10.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.2",
    "prettier": "~1.19.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "tsutils": "~3.17.0",
    "typescript": "^4.5.4"
  }
}
